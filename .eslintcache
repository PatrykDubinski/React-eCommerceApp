";\n\nconst App = (props) => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(checkUserSession());\n  }, []);\n\n  return (\n    <div className=\"App\">\n      <AdminToolbar />\n      <Switch>\n        <Route\n          exact\n          path=\"/\"\n          render={() => (\n            <HomepageLayout>\n              <Homepage />\n            </HomepageLayout>\n          )}\n        />\n        <Route\n          exact\n          path=\"/search\"\n          render={() => (\n            <MainLayout>\n              <Search />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/search/:filterType\"\n          render={() => (\n            <MainLayout>\n              <Search />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/product/:productID\"\n          render={() => (\n            <MainLayout>\n              <ProductDetails />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/registration\"\n          render={() => (\n            <MainLayout>\n              <Register />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/login\"\n          render={() => (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/recovery\"\n          render={() => (\n            <MainLayout>\n              <Recovery />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/dashboard\"\n          render={() => (\n            <WithAuth>\n              <DashboardLayout>\n                <Dashboard />\n              </DashboardLayout>\n            </WithAuth>\n          )}\n        />\n        <Route\n          path=\"/admin\"\n          render={() => (\n            <WithAdminAuth>\n              <AdminLayout>\n                <Admin />\n              </AdminLayout>\n            </WithAdminAuth>\n       