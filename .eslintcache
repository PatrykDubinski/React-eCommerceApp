\n    );\r\n  }\r\n\r\n  const configFilters = {\r\n    defaultValue: filterType,\r\n    options: [\r\n      {\r\n        name: \"Show all\",\r\n        value: \"\",\r\n      },\r\n      {\r\n        name: \"Mens\",\r\n        value: \"mens\",\r\n      },\r\n      {\r\n        name: \"Womens\",\r\n        value: \"womens\",\r\n      },\r\n    ],\r\n    handleChange: handleFilter,\r\n  };\r\n\r\n  const handleLoadMore = () => {\r\n    dispatch(\r\n      fetchProductsStart({\r\n        filterType,\r\n        startAfterDoc: queryDoc,\r\n        persistProducts: data,\r\n      })\r\n    );\r\n  };\r\n\r\n  const configLoadMore = {\r\n    onLoadMore: handleLoadMore,\r\n  };\r\n\r\n  return (\r\n    <div className=\"products\">\r\n      <h1>Browse Products</h1>\r\n\r\n      <FormSelect {...configFilters} />\r\n\r\n      <div className=\"productResults\">\r\n        {data.map((product, index) => {\r\n          const { productThumbnail, productName, productPrice } = product;\r\n\r\n          if (\r\n            !productThumbnail ||\r\n            !productName ||\r\n            typeof productPrice === \"undefined\"\r\n          )\r\n            return null;\r\n\r\n          const configProduct = {\r\n            ...product,\r\n          };\r\n\r\n          return <Product key={index} {...configProduct} />;\r\n        })}\r\n      </div>\r\n\r\n      {!isLastPage && <LoadMore {...configLoadMore} />}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ProductResults;\r\n","/mnt/c/Users/mrdub/Desktop/Learnin/ecommerce/src/components/ProductResults/Product/Product.js",[],"/mnt/c/Users/mrdub/Desktop/Learnin/ecommerce/src/components/LoadMore/LoadMore.js",[],"/mnt/c/Users/mrdub/Desktop/Learnin/ecommerce/src/pages/ProductDetails/ProductDetails.js",[],"/mnt/c/Users/mrdub/Desktop/Learnin/ecommerce/src/components/ProductCard/ProductCard.js",["235"],{"ruleId":"236","replacedBy":"237"},{"ruleId":"238","replacedBy":"239"},{"ruleId":"240","severity":1,"message":"241","line":33,"column":6,"no